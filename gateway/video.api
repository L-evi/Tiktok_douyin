syntax = "v1"

info (
    title: "video"
    desc: "video proc file"
)

type publishReq {
    Token string `form:"token"`
    //   data byte `form:"data"` // handler from l.r.FormFile
    Title string `form:"Title"`
}

@server(
    group: video
    middleware: Auth
)
service gateway {
    @handler publish
    post /douyin/public/action (publishReq)
}